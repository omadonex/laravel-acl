{
    "name": "omadonex/laravel-acl",
    "description": "Acl for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Arkhipov",
            "email": "omadonex@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "~8.0",
        "omadonex/laravel-support": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Omadonex\\LaravelAcl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omadonex\\LaravelAcl\\Providers\\AclServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-support"
        },
        {
            "type": "path",
            "url": "../laravel-acl"
        }
    ]
}
